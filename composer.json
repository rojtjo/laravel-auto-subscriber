{
    "name": "rojtjo/laravel-auto-subscriber",
    "description": "laravel-auto-subscriber",
    "keywords": [
        "rojtjo",
        "laravel-auto-subscriber"
    ],
    "homepage": "https://github.com/rojtjo/laravel-auto-subscriber",
    "license": "MIT",
    "authors": [
        {
            "name": "Roj Vroemen",
            "email": "me@rojvroemen.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/contracts": "^8.32|^9.2.0",
        "illuminate/support": "^8.32|^9.2.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.6.0",
        "phpunit/phpunit": "^9.5",
        "symfony/var-dumper": "^5.2",
        "vimeo/psalm": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "Rojtjo\\LaravelAutoSubscriber\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rojtjo\\LaravelAutoSubscriber\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
